{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/attached_assets/(.*)",
      "dest": "/attached_assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ],
  "functions": {
    "server/index.ts": {
      "includeFiles": ["attached_assets/**", "server/public/**", "dist/public/**"]
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}